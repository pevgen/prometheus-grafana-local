          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n    DATE_TRUNC('day', commit_date) AS day,\r\n    SUM(CASE WHEN login = 'user235' THEN changed_lines ELSE 0 END) AS user235,\r\n    SUM(CASE WHEN login = 'user2' THEN changed_lines ELSE 0 END) AS user2,\r\n    SUM(CASE WHEN login = 'pevgen' THEN changed_lines ELSE 0 END) AS pevgen\r\nFROM git_commits\r\nWHERE DATE_TRUNC('day', commit_date) BETWEEN $__timeFrom() AND $__timeTo()\r\nGROUP BY day\r\nORDER BY day;",
          "refId": "A",
          "sql": {
            "columns": [
